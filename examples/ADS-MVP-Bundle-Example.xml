				<about>
					<reference value="urn:uuid:835538cb-5722-41d2-a65e-5491ccb0b7e0"/>
				</about>
				<about>
					<reference value="urn:uuid:9450d479-328c-4c3c-bf35-efb1d80bd576"/>
